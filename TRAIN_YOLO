from ultralytics import YOLO

def main():
    # 加载预训练模型
    model = YOLO("LPRnet/models/weights/yolov8s.pt")
    # 开始训练
    model.train(
        data=r"F:\PYcharm\FILE\learn\configs\ccpd.yaml",
        epochs=100,
        batch=16,
        imgsz=640,
        device = 0,

    )

if __name__ == '__main__':
    main()
